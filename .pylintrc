[MESSAGES CONTROL]
disable=
    missing-class-docstring,
    too-few-public-methods,
    no-name-in-module,
    missing-module-docstring,
    missing-function-docstring,
    fixme,
    unspecified-encoding,
    missing-timeout,

enable=
    unused-import

[SIMILARITIES]
ignore-imports=yes
min-similarity-lines=7

[MASTER]
ignore=alembic alembic/versions alembic/env.py
